---CRIAÇÃO DE TABELAS---

DROP TABLE portal_sesmt.MOV_SETOR;

CREATE TABLE portal_sesmt.MOV_SETOR (

CD_HIS_MOV          INT NOT NULL,
CD_SETOR_MV_ANT     INT NOT NULL,
DT_MOVIMENTACAO     TIMESTAMP,
CD_SETOR_MV_ATUAL   INT NOT NULL,
CD_USUARIO          INT NOT NULL,
CD_USUARIO_CADASTRO VARCHAR(20) NOT NULL,
HR_CADASTRO         TIMESTAMP NOT NULL,
CD_USUARIO_ULT_ALT  VARCHAR(20),
HR_ULT_ALT          TIMESTAMP,

--PRIMARY_KEY
CONSTRAINT PK_CD_HIS_MOV PRIMARY KEY (CD_HIS_MOV));

--SEQUENCE 
DROP SEQUENCE portal_sesmt.SEQ_CD_HIS_MOV; 

CREATE SEQUENCE portal_sesmt.SEQ_CD_HIS_MOV
START WITH 1    
INCREMENT BY 1
NOCACHE
NOCYCLE;



